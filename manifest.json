{
  "manifest_version": 2,

  "name": "ChromeAdb",
  "version": "1.0.0",

  "app": {
   "background": {
     "scripts": ["app.js"]
    }
  },

  "permissions": [
    {
      "socket": ["tcp-connect:*:*"]
    },
    {
      "fileSystem": []
    }
  ],

  "sockets": {
    "tcp": {
      "connect": "*"
    }
  }
}
